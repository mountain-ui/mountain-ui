{
  "name": "@mountain-ui/doc-tools",
  "version": "0.2.0",
  "description": "Doc tools based on mountain ui",
  "keywords": [
    "mountain-ui",
    "react",
    "react-component",
    "components",
    "css-in-js",
    "styled",
    "styled-components",
    "styled-system"
  ],
  "homepage": "https://github.com/mountain-ui/mountain-ui/tree/main/packages/doc-tools#readme",
  "repository": "https://github.com/tonyghiani/mountain-ui",
  "license": "MIT",
  "author": "Marco Antonio Ghiani <marcoantonio.ghiani01@gmail.com>",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean:build && yarn build:vite && yarn build:types",
    "build:types": "tsc --project ./tsconfig.types.json",
    "build:vite": "vite build",
    "clean:build": "rimraf dist *.tsbuildinfo",
    "commit": "git cz",
    "lint": "eslint",
    "lint:package": "sort-package-json",
    "release": "semantic-release",
    "test": "NODE_ENV=testing jest --passWithNoTests"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "autoprefixer": ">=10",
    "postcss": ">= 8",
    "react": ">=18",
    "react-dom": ">=18",
    "tailwindcss": ">=3"
  },
  "publishConfig": {
    "access": "public"
  }
}
