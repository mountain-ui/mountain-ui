{
  "name": "@mountain-ui/icons",
  "version": "0.3.0",
  "description": "Icon components set for Mountain UI",
  "keywords": [
    "react",
    "react-components",
    "icons",
    "svg",
    "mountain-ui",
    "assets"
  ],
  "homepage": "https://github.com/mountain-ui/mountain-ui/tree/main/packages/icons#readme",
  "bugs": {
    "url": "https://github.com/mountain-ui/mountain-ui/issues"
  },
  "repository": "https://github.com/tonyghiani/mountain-ui",
  "license": "MIT",
  "author": "Marco Antonio Ghiani <marcoantonio.ghiani01@gmail.com>",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean:build && yarn build:vite && yarn build:types",
    "build:types": "tsc --project ./tsconfig.types.json",
    "build:vite": "vite build",
    "clean:build": "rimraf dist *.tsbuildinfo",
    "commit": "git cz",
    "convert": "svgr --out-dir src/icons --config-file ./svgr.config.js ./assets",
    "generate:types-map": "node ./lib/create-type-to-icon-map.js",
    "lint": "eslint",
    "lint:package": "sort-package-json",
    "test": "NODE_ENV=testing jest --passWithNoTests"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@mountain-ui/utils": "*",
    "@svgr/cli": "^8.1.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint-plugin-react": "^7.33.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "@mountain-ui/utils": "*",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "publishConfig": {
    "access": "public"
  }
}
